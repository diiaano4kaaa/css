<div class="row">
    <div class="col-3">
        <p class="text-secondary mb-0">{{number}}</p>
        <p>{{status}} </p>
    </div>
    <div class="col-2">
        <p class="text-secondary mb-0">{{amount.title}}</p>
        <p> {{amount.value}}
        </p>
    </div>
    <div class="col-3 m-1">
        {{#each images}}
        <img src="{{this}}" style="width:50px; height:50px;">
        {{/each}}
    </div>
    <div class="col-1">
        {{#with button}}
        {{#if isPrimary}}
        <a class="btn btn-primary" href="{{link}}">{{text}}</a>{{/if}}
        {{/with}}
    </div>
    <hr>
</div>