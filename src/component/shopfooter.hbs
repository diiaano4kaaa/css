<div class="row mx-4 mb-4 my-1 col-12">
    <hr>
    {{#each footer}}
    <div class="nav col-2">
        {{#each this}}
        <div>
            <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{this.link}}"> {{this.text}} </a>
        </div>
        {{/each}}
    </div>
    {{/each}}
</div>